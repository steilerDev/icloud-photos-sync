{
  "name": "icloud-photos-sync",
  "version": "0.0.0-development",
  "description": "One-way sync engine for the iCloud Photos Library into the native file system with archiving capabilities",
  "preferGlobal": true,
  "type": "module",
  "main": "bin/main.js",
  "bin": {
    "icloud-photos-sync": "bin/main.js"
  },
  "scripts": {
    "clean": "rm -rf bin/ coverage/ doc/ node_modules/ package-lock.json && find src/ -name \"*.schema.json\" -type f -delete",
    "lint": "npx eslint -c eslint.config.json --fix --ext .ts ./src ./test",
    "schema": "./build-schema.sh",
    "prebuild": "npm run lint && npm run schema",
    "build": "npx tsc",
    "typedoc": "npx typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme",
    "test": "NODE_NO_WARNINGS=1 NODE_OPTIONS='--experimental-vm-modules --experimental-json-modules' npx jest --config jest.config.json test/unit/",
    "test-single": "NODE_NO_WARNINGS=1 NODE_OPTIONS='--experimental-vm-modules --experimental-json-modules' npx jest --config jest.config.json",
    "test-api": "NODE_NO_WARNINGS=1 NODE_OPTIONS='--experimental-vm-modules --experimental-json-modules' npx jest --config jest.config.json test/api/",
    "execute": "node bin/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steilerDev/icloud-photos-sync"
  },
  "keywords": [
    "icloud",
    "icloud-sync",
    "icloud-photos"
  ],
  "author": "Frank Steiler",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/steilerDev/icloud-photos-sync/issues"
  },
  "homepage": "https://icloud-photos-sync.steilerdev.de/",
  "engines": {
    "node": ">=18.8.0"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/jest": "^29.5.2",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.16.19",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.5.0",
    "jest-summarizing-reporter": "^1.1.4",
    "mock-fs": "^5.2.0",
    "ts-jest": "^29.1.1",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.2.2",
    "backtrace-node": "^1.2.0",
    "chalk": "^5.0.0",
    "cli-progress": "^3.11.2",
    "commander": "^10.0.0",
    "croner": "^6.0.3",
    "loglevel": "^1.8.0",
    "p-event": "^5.0.1",
    "p-queue": "^7.2.0",
    "tough-cookie": "^4.0.0"
  }
}
